% QR Code

# Create a QR code with some content
echo <content> | curl -F-=\<- qrenco.de

% GPG
# Encrypt a file with GPG
gpg -c --armor --cipher-algo AES256 --no-symkey-cache <input_file>

# Decrypt a file with GPG
gpg --decrypt --no-symkey-cache --output <output_file> <input_file>

% FFmpeg
# Convert FLAC to ALAC
ffmpeg -i <input_file> -acodec alac <output_file_name>.m4a

$ input_file: ls -a